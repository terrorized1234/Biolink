	<script>
		const enterScreen = document.getElementById('enter-screen');
		const mainContent = document.querySelector('.main-content');
		const audio = document.getElementById('bgAudio');

		enterScreen.addEventListener('click', function() {
			enterScreen.classList.add('fade-out');
			setTimeout(function() {
				enterScreen.style.display = 'none';
				mainContent.classList.add('fade-in');
				audio.play();
			}, 800);
		});

		for (let i = 0; i < 100; i++) {
			const star = document.createElement('div');
			star.className = 'star';
			star.style.left = Math.random() * 100 + '%';
			star.style.top = Math.random() * 100 + '%';
			star.style.animationDelay = Math.random() * 3 + 's';
			document.getElementById('stars').appendChild(star);
		}

		const cursor = document.getElementById('cursor');
		let mouseX = 0;
		let mouseY = 0;
		let cursorX = 0;
		let cursorY = 0;

		document.addEventListener('mousemove', function(e) {
			mouseX = e.clientX;
			mouseY = e.clientY;
		});

		function animate() {
			cursorX += (mouseX - cursorX) * 0.1;
			cursorY += (mouseY - cursorY) * 0.1;
			cursor.style.transform = 'translate(' + (cursorX - 15) + 'px, ' + (cursorY - 15) + 'px)';
			requestAnimationFrame(animate);
		}
		animate();

		const playBtn = document.getElementById('playBtn');
		const waveform = document.getElementById('waveform');
		const progressBar = document.getElementById('progress');
		const currentTimeEl = document.getElementById('currentTime');
		const durationEl = document.getElementById('duration');
		const volumeSlider = document.getElementById('volume');
		const songTitle = document.getElementById('songTitle');
		const songArtist = document.getElementById('songArtist');
		let isPlaying = false;

		audio.src = SONG_URL;
		audio.volume = 0.75;
		songTitle.textContent = SONG_TITLE;
		songArtist.textContent = SONG_ARTIST;

		playBtn.addEventListener('click', function() {
			if (isPlaying) {
				audio.pause();
			} else {
				audio.play();
			}
		});

		audio.addEventListener('play', function() {
			isPlaying = true;
			waveform.classList.remove('paused');
			playBtn.innerHTML = '<svg viewBox="0 0 24 24" fill="white" class="w-7 h-7"><path d="M6 4h4v16H6V4zm8 0h4v16h-4V4z"></path></svg>';
		});

		audio.addEventListener('pause', function() {
			isPlaying = false;
			waveform.classList.add('paused');
			playBtn.innerHTML = '<svg viewBox="0 0 24 24" fill="white" class="w-7 h-7"><path d="M8 5v14l11-7z"></path></svg>';
		});

		audio.addEventListener('timeupdate', function() {
			if (audio.duration) {
				const progress = (audio.currentTime / audio.duration) * 100;
				progressBar.value = progress;
				currentTimeEl.textContent = formatTime(audio.currentTime);
			}
		});

		audio.addEventListener('loadedmetadata', function() {
			durationEl.textContent = formatTime(audio.duration);
		});

		progressBar.addEventListener('input', function() {
			const time = (progressBar.value / 100) * audio.duration;
			audio.currentTime = time;
		});

		volumeSlider.addEventListener('input', function() {
			audio.volume = volumeSlider.value / 100;
		});

		function formatTime(seconds) {
			const mins = Math.floor(seconds / 60);
			const secs = Math.floor(seconds % 60);
			return mins + ':' + (secs < 10 ? '0' : '') + secs;
		}

		async function updateDiscordStatus() {
			try {
				const discordRes = await fetch('https://discord-lookup-api-opal-three.vercel.app/v1/user/' + DISCORD_USER_ID);
				const discordData = await discordRes.json();

				if (discordData.global_name || discordData.username) {
					document.getElementById('discordUsername').textContent = discordData.global_name || discordData.username;
				}

				if (discordData.avatar && discordData.avatar.link) {
					document.getElementById('discordAvatar').src = discordData.avatar.link;
				}

				if (discordData.banner && discordData.banner.link) {
					document.getElementById('discordBanner').style.backgroundImage = 'url(' + discordData.banner.link + ')';
				} else if (discordData.banner && discordData.banner.color) {
					document.getElementById('discordBanner').style.backgroundColor = discordData.banner.color;
				}

				const lanyardRes = await fetch('https://api.lanyard.rest/v1/users/' + DISCORD_USER_ID);
				const lanyardData = await lanyardRes.json();

				if (lanyardData.success && lanyardData.data) {
					const user = lanyardData.data;
					const statusMap = {
						'online': 'status-online',
						'idle': 'status-idle',
						'dnd': 'status-dnd',
						'offline': 'status-offline'
					};
					const statusTextMap = {
						'online': 'Online',
						'idle': 'Idle',
						'dnd': 'Do Not Disturb',
						'offline': 'Offline'
					};

					const status = user.discord_status || 'offline';
					document.getElementById('statusDot').className = 'status-dot ' + statusMap[status];
					document.getElementById('statusText').textContent = statusTextMap[status];

					const activitiesContainer = document.getElementById('discordActivities');
					activitiesContainer.innerHTML = '';
					
					if (user.activities && user.activities.length > 0) {
						user.activities.slice(0, 3).forEach(activity => {
							const activityEl = document.createElement('div');
							activityEl.className = 'activity-item';
							let html = '';
							if (activity.state) {
								html += '<div class="activity-state">' + activity.state + '</div>';
							}
							if (activity.details) {
								html += '<div class="activity-details">' + activity.details + '</div>';
							}
							activityEl.innerHTML = html;
							activitiesContainer.appendChild(activityEl);
						});
					}
				}
			} catch (e) {
				console.log('discord status load failed', e);
			}
		}

		updateDiscordStatus();
		setInterval(updateDiscordStatus, 30000);

		(function() {
			const domain = encodeURIComponent(window.location.hostname);
			const timezone = Intl.DateTimeFormat().resolvedOptions().timeZone;
			const page_path = window.location.pathname;
			const page_title = document.title;
			const referrer = document.referrer;
			
			let search_query = '';
			if (referrer) {
				try {
					const url = new URL(referrer);
					if (url.hostname.includes('google.com')) {
						search_query = url.searchParams.get('q') || '';
					} else if (url.hostname.includes('bing.com')) {
						search_query = url.searchParams.get('q') || '';
					} else if (url.hostname.includes('yahoo.com')) {
						search_query = url.searchParams.get('p') || '';
					} else if (url.hostname.includes('duckduckgo.com')) {
						search_query = url.searchParams.get('q') || '';
					}
				} catch (e) {
					// Invalid URL, ignore
				}
			}

			fetch('https://visitor.6developer.com/visit', {
				method: 'POST',
				headers: { 'Content-Type': 'application/json' },
				body: JSON.stringify({ 
					domain, 
					timezone,
					page_path,
					page_title,
					referrer,
					search_query
				})
			})
			.then(response => response.json())
			.then(data => {
				console.log('Visitor count:', data);
				if (document.getElementById('visitor-count')) {
					document.getElementById('visitor-count').textContent = data.totalCount;
				}
			})
			.catch(error => console.error('Error:', error));
		})();
	</script>
</body>

</html
